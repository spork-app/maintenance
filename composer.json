{
    "name": "spork/maintenance",
    "description": "The maintenance package for Spork",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Austin Kregel",
            "email": "5355937+austinkregel@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Spork\\Maintenance\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spork\\Maintenance\\MaintenanceServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spork\\Maintenance\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "homepage": "https://github.com/spork-app/greenhouse",
    "keywords": [
        "spork-plugins"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}